<div class="w3" style="background-color:#84b8FB;max-width:2000px;margin-right:-15px;margin-left:-15px" id="quickquiz">
    <h2 class="w3-wide w3-center w3-padding-16">Let Us Understand You Before Suggesting</h2>
    <div class="w3-row-padding w3-center">
        <div class="w3-card w3-container" style="min-height:300px;background-color:white;">
            <div id="questions" style="padding:32px">

                <div class="question active">
                    <div class="progress">

                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="16.7"
                             aria-valuemin="0" aria-valuemax="100" style="width:16.7%;text-align:center">
                            Question 1 of 6

                        </div>

                    </div>Which season are you concern about?
                    <br>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="winter">Cold Weather</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="summer">Hot Weather</button></p>
                </div>
                <div class="question">
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="33.4"
                             aria-valuemin="0" aria-valuemax="100" style="width:33.4%;text-align:center">
                            Question 2 of 6
                        </div>
                    </div>
                    Which type of house you are living in?
                    <br>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="fua">Flat or Unit or Apartment</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="srtt">Semi-detached or Row or Terrace or Townhouse</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="sh">Seperate House</button></p>
                </div>
                <div class="question">
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="50"
                             aria-valuemin="0" aria-valuemax="100" style="width:50%;text-align:center">
                            Question 3 of 6
                        </div>
                    </div>

                    <div id="winterheat" style="display:none">
                        Which type of heating system you using?
                        <br>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="ductedgas"> Ducted gas</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="rvcacs">Reverse- cycle air-conditioning</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="wallmge">Wall-mounted Gas or Wall-mounted Electric</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="portableelecgas">Portable electric or Gas</button></p>
                    </div>
                    <div id="summercool" style="display:none">
                        Which type of cooling system you using?
                        <br>
                        <br />
                        <p><button class="btn-responsive btn-block" type="button" id="rvcacw">Reverse- cycle air-conditioning</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="evaporativecool">Evaporative cooler</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="wallwinmount">Wall or Window Mounted Air Conditioner</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="portac">Portable Air-Conditioning</button></p>
                    </div>
                </div>
                <div class="question">
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="66.7"
                             aria-valuemin="0" aria-valuemax="100" style="width:66.7%;text-align:center">
                            Question 4 of 6
                        </div>
                    </div>
                    Do you like colder or warmer temperature compare to others?
                    <br>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="colderc">Colder</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="normalc">Normal</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="warmerc">Warmer</button></p>
                    <br>
                </div>
                <div class="question">
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="83.33"
                             aria-valuemin="0" aria-valuemax="100" style="width:83.33%;text-align:center">
                            Question 5 of 6
                        </div>
                    </div>
                    Are you living with others?
                    <br>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="liveyes">Yes</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="liveno">No</button></p>
                    <br>
                    <div id="liveothers" style="display:none">
                        Do they like colder or warmer compare to you?
                        <br>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="coldero">Colder</button></p>
                        <br>
                        <p><button class="btn-responsive btn-block" type="button" id="warmero">Warmer</button></p>
                        <br>
                    </div>
                </div>
                <div class="question">
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="100"
                             aria-valuemin="0" aria-valuemax="100" style="width:100%;text-align:center">
                            Question 6 of 6
                        </div>
                    </div>
                    <div id="lastq">
                        How usually do your house expose to the sunlight from glass window?
                    </div>
                    <br>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="more6">More than 6 hours</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="3to6hrs">3 to 6 hours</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="1to3hrs">1 to 3 hours</button></p>
                    <br>
                    <p><button class="btn-responsive btn-block" type="button" id="lessthan1">Less than 1 hour</button></p>
                    <br>
             
                </div>

            </div>
            <div id="result" style="display:none;margin-top:5%">
                <h1> Thank you for taking your time with us!</h1>
                <h2><b>Our Suggestion to you</b></h2>

                <br>
                <h4><b>Based on your answer</b></h4>
                <p>Set your themostat to: </p>
                <p id="temperaturesetper"></p>
                <br>
                <p>Length of time:</p>
                <p id="hoursetper"></p>
                <br>
                <h4><b>Standard</b></h4>
                <p id="coolset" style="display:none">Setting themostat 8°C higher than outside temperature for average 6 hours</p>
                <p id="heatset" style="display:none">Setting themostat 8°C lower than outside temperature for average 6 hours</p><br><br>
            </div>

        </div>

        <br><br>
        <br>
    </div>
</div>

<script>
    $('#prevyr').click(function () {
        document.getElementById('prevyear').style.display = 'initial';
        document.getElementById('curyear').style.display = 'none';
    });

     $('#curyr').click(function () {
        document.getElementById('prevyear').style.display = 'none';
        document.getElementById('curyear').style.display = 'initial';
    });
    var season;
    var roomtype;
    var cooltype;
    var heattype;
    var personalperfer;
    var livewithother;
    var otherprefer;
    var exposure;

    $('#winter').click(function () {
        document.getElementById('winterheat').style.display = 'initial';
        document.getElementById('heatset').style.display = 'initial';
        var $winter = $('.question.active');
        $('.question').removeClass('active');
        $winter.next().addClass('active');
        season = 0;
        return season;

    });
    //season 0=winter,1=summer
    $('#summer').click(function () {
        document.getElementById('coolset').style.display = 'initial';
        document.getElementById('summercool').style.display = 'initial';
        var $summer = $('.question.active');
        $('.question').removeClass('active');
        $summer.next().addClass('active');
        season = 1;
        return season;
    });

    $('#fua').click(function () {
        var $fua = $('.question.active');
        $('.question').removeClass('active');
        $fua.next().addClass('active');
        roomtype = 0;
        return roomtype;
    });

    $('#srtt').click(function () {
        var $srtt = $('.question.active');
        $('.question').removeClass('active');
        $srtt.next().addClass('active');
        roomtype = 1;
        return roomtype;
    });

    $('#sh').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        roomtype = 2;
        return roomtype;
    });

    $('#ductedgas').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        heattype = 0;
        return heattype;
    });

    $('#rvcacs').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        heattype = 1;
        return heattype;
    });

    $('#wallmge').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        heattype = 2;
        return heattype;
    });

    $('#portableelecgas').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        heattype = 3;
        return heattype;
    });

    $('#rvcacw').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        cooltype = 0;
        return cooltype;
    });

    $('#evaporativecool').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        cooltype = 1;
        return cooltype;
    });

    $('#wallwinmount').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        cooltype = 2;
        return cooltype;
    });


    $('#portac').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        cooltype = 3;
        return cooltype;
    });

    $('#colderc').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        personalperfer = 0;
        return personalperfer;
    });

    $('#normalc').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        personalperfer = 1;
        return personalperfer;
    });

    $('#warmerc').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        personalperfer = 2;
        return personalperfer;
    });

    $('#liveyes').click(function () {
        document.getElementById('liveothers').style.display = 'initial';
        document.getElementById('liveno').style.display = 'none';
        livewithother = 0;
        return livewithother;
    });

    $('#liveno').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        livewithother = 1;
        return livewithother;
    });

    $('#coldero').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        otherprefer = 0;
        return otherprefer;
    });

    $('#warmero').click(function () {
        var $sh = $('.question.active');
        $('.question').removeClass('active');
        $sh.next().addClass('active');
        otherprefer = 1;
        return otherprefer;
    });

    $('#more6').click(function () {

        exposure = 0;
          document.getElementById('questions').style.display = 'none';
        document.getElementById('result').style.display = 'initial';

        if (season == 0) {
            var temperatureset = -8;
            var hourset = 6;

            if (roomtype == 0) { hourset = hourset - 1 }
            if (roomtype == 1) { hourset = hourset }
            if (roomtype == 2) { hourset = hourset + 1 }
            if (heattype == 0) { temperatureset = temperatureset - 2 }
            if (heattype == 1) { temperatureset = temperatureset + 1 }
            if (heattype == 2) { temperatureset = temperatureset - 1 }
            if (heattype == 3) { temperatureset = temperatureset }
            if (personalperfer == 0) { temperatureset = temperatureset - 2 }
            if (personalperfer == 1) { temperatureset = temperatureset }
            if (personalperfer == 2) { temperatureset = temperatureset + 2 }
            if (livewithother == 0) {
                if (otherprefer == 0) { temperatureset = temperatureset - 1 }
                if (otherprefer == 1) { temperatureset = temperatureset + 1 }
            }
            if (exposure == 0) { hourset = hourset - 2 }
            if (exposure == 1) { hourset = hourset - 1 }
            if (exposure == 2) { hourset = hourset }
            if (exposure == 3) { hourset = hourset + 2 }
            document.getElementById("hoursetper").innerHTML = hourset + " hours";
            document.getElementById("temperaturesetper").innerHTML = temperatureset + " °C compare to outdoor temperature";
        }


        if (season == 1) {
            var temperatureset = 8;
            var hourset = 6;
            if (roomtype == 0) { hourset = hourset + 1 }
            if (roomtype == 1) { hourset = hourset }
            if (roomtype == 2) { hourset = hourset - 1 }
            if (heattype == 0) { temperatureset = temperatureset + 2 }
            if (heattype == 1) { temperatureset = temperatureset - 1 }
            if (heattype == 2) { temperatureset = temperatureset + 1 }
            if (heattype == 3) { temperatureset = temperatureset }
            if (personalperfer == 0) { temperatureset = temperatureset - 2 }
            if (personalperfer == 1) { temperatureset = temperatureset }
            if (personalperfer == 2) { temperatureset = temperatureset + 2 }
            if (livewithother == 0) {
                if (otherprefer == 0) { temperatureset = temperatureset - 1 }
                if (otherprefer == 1) { temperatureset = temperatureset + 1 }
            }
            if (exposure == 0) { hourset = hourset + 2 }
            if (exposure == 1) { hourset = hourset + 1 }
            if (exposure == 2) { hourset = hourset }
            if (exposure == 3) { hourset = hourset - 2 }
            document.getElementById("hoursetper").innerHTML = hourset + " hours";
            document.getElementById("temperaturesetper").innerHTML = temperatureset + " °C compare to outdoor temperature";
            return exposure;
        }
    });

    $('#3to6hrs').click(function () {

        exposure = 1;
          document.getElementById('questions').style.display = 'none';
        document.getElementById('result').style.display = 'initial';

        if (season == 0) {
            var temperatureset = -8;
            var hourset = 6;

            if (roomtype == 0) { hourset = hourset - 1 }
            if (roomtype == 1) { hourset = hourset }
            if (roomtype == 2) { hourset = hourset + 1 }
            if (heattype == 0) { temperatureset = temperatureset - 2 }
            if (heattype == 1) { temperatureset = temperatureset + 1 }
            if (heattype == 2) { temperatureset = temperatureset - 1 }
            if (heattype == 3) { temperatureset = temperatureset }
            if (personalperfer == 0) { temperatureset = temperatureset - 2 }
            if (personalperfer == 1) { temperatureset = temperatureset }
            if (personalperfer == 2) { temperatureset = temperatureset + 2 }
            if (livewithother == 0) {
                if (otherprefer == 0) { temperatureset = temperatureset - 1 }
                if (otherprefer == 1) { temperatureset = temperatureset + 1 }
            }
            if (exposure == 0) { hourset = hourset - 2 }
            if (exposure == 1) { hourset = hourset - 1 }
            if (exposure == 2) { hourset = hourset }
            if (exposure == 3) { hourset = hourset + 2 }
            document.getElementById("hoursetper").innerHTML = hourset + " hours";
            document.getElementById("temperaturesetper").innerHTML = temperatureset + " °C compare to outdoor temperature";
        }


        if (season == 1) {
            var temperatureset = 8;
            var hourset = 6;
            if (roomtype == 0) { hourset = hourset + 1 }
            if (roomtype == 1) { hourset = hourset }
            if (roomtype == 2) { hourset = hourset - 1 }
            if (heattype == 0) { temperatureset = temperatureset + 2 }
            if (heattype == 1) { temperatureset = temperatureset - 1 }
            if (heattype == 2) { temperatureset = temperatureset + 1 }
            if (heattype == 3) { temperatureset = temperatureset }
            if (personalperfer == 0) { temperatureset = temperatureset - 2 }
            if (personalperfer == 1) { temperatureset = temperatureset }
