<style>
    .grid-container {
        display: grid;
        max-width: 2000px;
        grid-template-columns: 14% 14% 14% 14% 14% 14% 14%;
        grid-gap: 1px;
    }


        .grid-container > div {
            text-align: center;
            font-size: 10px;
        }

    .question:not(.active) {
        display: none;
    }

    @@media (max-width: 768px) {
        .btn-responsive {
            padding: 2px 4px;
            font-size: 80%;
            line-height: 1;
        }
    }

    @@media (min-width: 769px) and (max-width: 992px) {
        .btn-responsive {
            padding: 4px 9px;
            font-size: 90%;
            line-height: 1.2;
            max-width: 400px;
        }
    }
</style>
